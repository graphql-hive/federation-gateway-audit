{
  "name": "federation-compatibility",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --port 4200 src/index.ts",
    "gateway": "node --import tsx/esm --watch --watch-path=./src --watch-path=./gateway.ts ./gateway.ts",
    "gateway:once": "node --import tsx/esm ./gateway.ts",
    "test": "tsx ./test.ts",
    "test-all": "tsx ./test-all.ts",
    "deploy": "wrangler deploy --minify src/index.ts"
  },
  "dependencies": {
    "@apollo/subgraph": "2.7.4",
    "@apollo/gateway": "2.7.4",
    "@apollo/query-planner": "2.7.4",
    "graphql-yoga": "5.3.0",
    "graphql": "16.8.1",
    "fets": "0.8.0"
  },
  "devDependencies": {
    "jest-diff": "29.7.0",
    "tsx": "4.7.2",
    "@cloudflare/workers-types": "4.20240405.0",
    "wrangler": "3.51.2",
    "@apollo/composition": "2.7.4",
    "wait-on": "7.2.0"
  }
}
