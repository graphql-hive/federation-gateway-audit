{
  "name": "graphql-federation-audit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Audit tool for Apollo Federation Gateway",
  "repository": {
    "type": "git",
    "url": "the-guild-org/federation-compatibility"
  },
  "homepage": "https://the-guild.dev/",
  "author": {
    "email": "contact@the-guild.dev",
    "name": "The Guild",
    "url": "https://the-guild.dev"
  },
  "license": "MIT",
  "bin": {
    "graphql-federation-audit": "./dist/cli.js"
  },
  "main": "dist/cli.js",
  "module": "dist/cli.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "scripts": {
    "start": "tsx ./src/cli.ts",
    "summary": "tsx ./src/summary.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apollo/composition": "2.9.0",
    "@apollo/subgraph": "2.9.0",
    "async-retry": "1.3.3",
    "dotenv": "16.4.5",
    "fets": "0.8.1",
    "get-port": "7.1.0",
    "graphql": "16.9.0",
    "graphql-yoga": "5.6.1",
    "jest-diff": "29.7.0",
    "kill-port-process": "3.2.1",
    "wait-on": "7.2.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@apollo/gateway": "2.9.0",
    "@apollo/server": "4.11.0",
    "@types/async-retry": "1.4.8",
    "@types/express": "4.17.21",
    "@types/node": "20.14.12",
    "@types/wait-on": "5.3.4",
    "@types/yargs": "17.0.32",
    "express": "4.19.2",
    "prettier": "3.3.3",
    "tsx": "4.15.7",
    "typescript": "5.5.4",
    "wgc": "0.63.0"
  }
}
